<% users.forEach((user) => { %>
  <div class="row user">
    <div class="col-md-1 offset-md-3">
      <div class="image" style="background-image:url(<%= user.imageSRC %>)" alt="<%= user.name %>"></div>
    </div>
    <div class="col-md-8">
      <a href="/users/<%= user.id %>/<%=user.pylons%>"><h2><%= user.name %></h2></a>
    </div>
  </div>
<% }) %>
